{
  "name": "fueleu-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/infrastructure/server/dev-server.ts",
    "build": "tsc -p .",
    "start": "node dist/infrastructure/server/server.js",
    "migrate": "prisma migrate dev --name init",
