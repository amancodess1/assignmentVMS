  },
  "dependencies": {
    "@prisma/client": "^5.5.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.2.5",
    "@types/supertest": "^6.0.3",
    "prisma": "^5.5.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0",
    "vitest": "^0.34.0"
  }
}
